<?xml version="1.0"?>
<classes>

  <!-- abbreviation=phavu, etc. -->
  <class name="Organism" is-interface="true">
    <attribute name="abbreviation" type="java.lang.String"/>
  </class>

  <!-- Protein, CDS and Transcript loaded from files with "primaryTranscript" in the file name get flagged with primaryTranscript=true -->
  <!-- HOWEVER this has to be attached to BioEntity because that's what the FASTA loader operates on -->
  <class name="BioEntity" is-interface="true">
    <attribute name="primaryTranscript" type="java.lang.Boolean"/>
  </class>

</classes>
